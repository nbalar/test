@using VenomitIT.Common;
@using VenomitIT.BLL;
@model VenomitIT.DAL.EditDomainModel
@{
    ViewBag.Title = "EditDomain";    
}
<form method="post" id="frmdomain">
    @Html.HiddenFor(model => model.DomainId)
    <div class="form_area form_area_01">
        <ul>

            <li>
                <label>
                    Domain Name :</label>
                @Html.TextBoxFor(x => x.DomainName)
                @Html.ValidationMessageFor(x => x.DomainName)
            </li>
            <li>
                <label>
                    Domain URL :</label>
                @Html.TextBoxFor(x => x.DomainURL)
                @Html.ValidationMessageFor(x => x.DomainURL)
            </li>
            <li>
                <label>
                    Is Active :</label>
                @Html.CheckBoxFor(model => model.IsActive)
            </li>
        </ul>
    </div>
    <div class="btn_box23">
        <input type="submit" value="" id="btnSave" class="save_btn" />
        <div class="p_detail_btn_box p_detail_btn_box_01" style="width: 137px !important;">
            <div class="left_box">
                <input type="button" value="" id="btncancel" onclick="Back()" class="cancel_btn" />
            </div>



        </div>
    </div>
</form>
<script>
    function Back() {
        window.location = '@VenomitIT.Common.NavigationURL.ManageDomain';
    }
</script>
